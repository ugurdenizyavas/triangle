<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean name="authorizationService"
		class="com.deniz.framework.security.authorization.AuthorizationService">
		<property name="hostBase" value="${authorization.hostbase}" />
		<property name="hostPort" value="${authorization.hostport}" />
	</bean>

</beans>